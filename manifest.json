{
	"name": "Automate mute",
	"version": "2.1.0",
	"description": "Rozszerzenie automatycznie wycisza wszystkich użytkowników konferencjii poza moderatorami.",
	"permissions": ["activeTab", "tabs", "storage"],
	"browser_action": {
		"default_popup": "popup.html"
	},
	"web_accessible_resources": [
		"scripts/popup.js",
		"scripts/settings.js"
	],
	"content_scripts": [{
		"matches": ["https://ipfon24.ipfon.pl/?action=konferencja_static&id=a00e2caa5a6ec313216b59c937dccf65"],
		"js": ["scripts/contentScript.js"],
		"css": ["styles/contentStylesheet.css"]
	}],
	"options_page": "settings.html",
	"manifest_version": 2
}